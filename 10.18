using System;
 
class Program {
  public static void Main (string[] args) {
  string number;
  int grade;
    Console.WriteLine("Enter your grade");
    number = Console.ReadLine();
    grade = Convert.ToInt32(number);
    if(grade > 80) {
      Console.WriteLine("Grade is Above 80");
    }
    else if(grade > 70) {
      Console.WriteLine("Above 70 but less than 80");
    }
    else if(grade > 60) {
      Console.WriteLine("Above 60 but less than 70");
    }
    else{
      Console.WriteLine("Fail");
    }
  }
}
